{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "baseUrl": ".",
    "paths": {
      "@/src/*": [
        "src/*"
      ],
      "@/themes/*": [
        "themes/*"
      ]
    }
  },
  "include": [
    "./src"
  ],
  "exclude": [
    "node_modules"
  ]
}
